@model CoursePhase1

        <div class="card">
            <div class="card-title">
                <h6>BWG Attended</h6>
                <img src="~/Images/icons8-plus-24.png" class="img-add" id="add_course" />
            </div>
            <div class="card-body">
                <div id="add_courser_1">
                    <div class="div-cancel-icon">
                        <img src="~/Images/icons8-cancel-24.png" id="cancel_add_course_1" class="cancel-icon" />
                    </div>
                    <div class="row">
                        <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12">
                            <div class="form-floating mb-3">
                                @Html.TextBoxFor(e => e.AddCourseFrom, new { @class = "form-control date-picker", id = "txt_addcourse_from_1", placeholder = "From", autocomplete = "none" })
                                @*@Html.LabelFor(e => e.AddCourseFrom, new { @for = "txt_addcourse_from" })*@
                                <label for="txt_addcourse_from_1" class="date-picker">From</label>
                                @Html.ValidationMessageFor(e => e.AddCourseFrom,"",new {@class="validation-message"})
                                <span class="spn-validation-error"></span>
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12">
                            <div class="form-floating mb-3">
                                @Html.TextBoxFor(e => e.AddCourseTo, new { @class = "form-control date-picker", id = "txt_addcourse_to_1", placeholder = "To", autocomplete = "none" })
                                @*@Html.LabelFor(e => e.AddCourseTo, new { @for = "txt_addcourse_to" })*@
                                <label for="txt_addcourse_to_1" class="date-picker">To</label>
                                @Html.ValidationMessageFor(e => e.AddCourseTo,"",new {@class="validation-message"})
                                <span class="spn-validation-error"></span>
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12">
                            <div class="form-floating mb-3">
                                @Html.TextBoxFor(e => e.AddCoursePlace, new { @class = "form-control", id = "txt_addcourse_place_1", placeholder = "Place", autocomplete = "none", @onkeypress = "fnAlphabetWithSpace(event)" })
                                <label for="txt_addcourse_place_1">Place</label>
                                @Html.ValidationMessageFor(e => e.AddCoursePlace,"",new {@class="validation-message"})
                                <span class="spn-validation-error"></span>
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12">
                            <div class="form-floating mb-3">
                                @Html.TextBoxFor(e => e.ParentTeachersName, new { @class = "form-control", id = "txt_addcourse_parent_teacher_name_1", placeholder = "Parent Teacher's Name", autocomplete = "none", @onkeypress = "fnAlphabetWithSpace(event)" })
                                <label for="txt_addcourse_parent_teacher_name_1">Parent Teachers Name</label>
                                @Html.ValidationMessageFor(e => e.ParentTeachersName,"",new {@class="validation-message"})
                                <span class="spn-validation-error"></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="div-course">

                </div>

            </div>
        </div>













